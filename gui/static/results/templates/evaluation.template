<table id = "summary_table" class="fancy_table">
	<thead>
		<tr>
			<th><b>Clustering</b></th>
			{{#evaluation_tags}}
			<th><b>{{tag}}</b></th>
		    {{/evaluation_tags}}
		</tr>
	</thead>
	<tbody>
		{{#evaluations}}
		<tr {{#if best_clustering}} class ="best_clustering" {{/if}}>
			<td class ="details"> {{id}} </td>
			{{#values}}
				<td > {{formatted value}} </td>
			{{/values}}
		</tr>
		{{/evaluations}}
	</body>
</table>

<div class = "green_square"> <div class="green_square_text"> Best clustering </div> </div>

<table id="criteria_table" class="fancy_table" style = "width:auto;">
	<thead>
		<tr>
			<th class="small_width""> <b>Criteria</b> </th>
			{{#criteria_table_headers}}
			<th class="small_width"> <b>{{id}}</b> </th>
		    {{/criteria_table_headers}}
		</tr>
	</thead>
	<tbody>
		{{#criteria_table}}
		<tr>
			<td class="details small_width" > {{id}} </td>
			{{#values}}
				<td class="small_width"> {{formatted value}} </td>
			{{/values}}
		</tr>
		{{/criteria_table}}
	</body>
</table>